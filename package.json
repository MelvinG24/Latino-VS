{
    "name": "latino-vscode",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "1.0.1",
	"publisher": "MelvinGuerrero",
    "license": "MIT",
    "author": {
		"email": "melvin_guerrero@yahoo.com",
		"name": "Melvin Guerrero",
		"url": "https://melvinguerrero.blogspot.com"
	},
    "engines": { "vscode": "^1.0.0" },
    "icon": "images/Latino-Logo.png",
    "categories":[
        "Programming Languages",
        "Language Packs",
        "Snippets"
    ],
    "keywords": [
		"latino",
		"lenguaje",
		"language"
	],
    "galleryBanner":{
        "color": "#449eec",
        "theme": "dark"
    },
    "bugs": {
        "url": "https://github.com/MelvinG24/Latino-VSCode/issues",
        "email": "melvin_guerrero@yahoo.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MelvinG24/Latino-VSCode.git"
    },
    "homepage": "https://github.com/MelvinG24/Latino-VSCode",
    "activationEvents": ["*"],
    "main": "images/iconsFiles/latino-icons.bundle.js",
    "contributes": {
        "iconThemes":
        [
            {
                "id": "latino",
				"label": "Latino Icon",
				"path": "images/iconsFiles/latino-icon-theme.json",
				"_watch": true
			}
		],
        "languages":
        [
            {
                "id": "latino",
                "extensions": [".lat"],
                "aliases": ["Latino", "latino"],
                "configuration": "language-configuration.json"
            }
        ],
        "grammars":
        [
            {
                "language": "latino",
                "scopeName": "source.latino",
                "path": "syntaxes/latino.tmLanguage.json"
            }
        ],
        "snippets":
        [
            {
                "language": "latino",
                "path": "snippets/latino.snippets.json"
            }
        ]
    }
}