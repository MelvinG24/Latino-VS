{
    "name": "latino-vscode",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "1.0.0",
	"publisher": "Melvin-Guerrero",
	"license": "MIT",
    "engines": { "vscode": "*" },
    "icon": "images/Latino-Logo.png",
    "categories":[
        "Programming Languages",
        "Language Packs",
        "Snippets"
    ],
    "author": {"name": "Melvin Guerrero"},
    "galleryBanner":{
        "color": "#449eec",
        "theme": "dark"
    },
    "bugs": {
        "url": "https://github.com/MelvinG24/Latino-VSCode/issues",
        "email": "melving24@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MelvinG24/Latino-VSCode.git"
    },
    "homepage": "https://github.com/MelvinG24/Latino-VSCode",
    "contributes": {
        "languages":
        [
            {
                "id": "latino",
                "extensions": [".lat"],
                "aliases": ["Latino", "latino"],
                "configuration": "language-configuration.json"
            }
        ],
        "grammars":
        [
            {
                "language": "latino",
                "scopeName": "source.latino",
                "path": "syntaxes/latino.tmLanguage.json"
            }
        ],
        "snippets":
        [
            {
                "language": "latino",
                "path": "snippets/latino.snippets.json"
            }
        ]
    }
}