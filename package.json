{
    "name": "latino-vscode",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "1.0.0",
	"publisher": "Melvin_Guerrero",
	"license": "MIT",
    "engines": { "vscode": "*" },
    "icon": "https://github.com/MelvinG24/Latino-VS/blob/master/images/Latino-Logo.png",
    "contributes": {
        "languages":
        [
            {
                "id": "latino",
                "extensions": [".lat"],
                "aliases": ["Latino", "latino"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars":
        [
            {
                "language": "latino",
                "scopeName": "source.latino",
                "path": "./syntaxes/latino.tmLanguage.json"
            }
        ],
        "snippets":
        [
            {
                "language": "latino",
                "path": "./snippets/latino.snippets.json"
            }
        ]
    }
}