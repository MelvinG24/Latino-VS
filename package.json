{
    "name": "Latino-VS",
    "displayName": "Lenguaje Latino",
    "description": "Sintaxis de Latino para VS Code",
    "icon": "images/Latino-Logo.png",
    "version": "1.0",
    "publisher": "MelvinGuerrero",
    "author": {
        "name": "Melvin Guerrero",
        "email": "melvin_guerrero@yahoo.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MelvinG24/Latino-VS"
    },
    "keywords": [
        "Latino",
        "sintax",
    ],
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "Latino",
            "aliases": ["Latino", "Latino-VS"],
            "extensions": [".lat"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "Latino",
            "scopeName": "source.latino",
            "path": "./syntaxes/Latino.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "Latino",
                "path": "./snippets/snippets.json"
            }
        ],
        "configurationDefaults": {
            "[lat]": {
                "files.encoding": "cp437",
                "files.insertFinalNewline": true
            }
        }
    }
}