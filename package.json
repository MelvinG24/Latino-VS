{
    "name": "latino-vscode",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "1.0.1",
    "publisher": "MelvinGuerrero",
    "license": "MIT",
    "author": {
        "email": "melvin_guerrero@yahoo.com",
        "name": "Melvin Guerrero",
        "url": "https://melvinguerrero.blogspot.com"
    },
    "engines": { "vscode": "^1.0.0" },
    "icon": "images/Latino-Logo.png",
    "categories":[
        "Debuggers",
        "Language Packs",
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "latino",
        "lenguaje",
        "language"
    ],
    "galleryBanner":{
        "color": "#449eec",
        "theme": "dark"
    },
    "bugs": {
        "url": "https://github.com/MelvinG24/Latino-VSCode/issues",
        "email": "melvin_guerrero@yahoo.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MelvinG24/Latino-VSCode.git"
    },
    "homepage": "https://github.com/MelvinG24/Latino-VSCode",
    "activationEvents": [
        "onLanguage:latino"
    ],
    "main": "./out/client/extension",
    "scripts":{
        "compile":"",
        "vscode:uninstall": "node ./out/src/lifecycle"
    },
    "contributes": {
        "languages":
        [
            {
                "id": "latino",
                "extensions": [".lat"],
                "aliases": ["Latino", "latino"],
                "configuration": "language-configuration.json"
            }
        ],
        "grammars":
        [
            {
                "language": "latino",
                "scopeName": "source.latino",
                "path": "syntaxes/latino.tmLanguage.json"
            }
        ],
        "snippets":
        [
            {
                "language": "latino",
                "path": "snippets/latino.snippets.json"
            }
        ]
    }
}